{
  "name": "@jarvis/vscode-client",
  "displayName": "Jarvis VSCode Client",
  "description": "JavaScript intellisense based on tree-sitter.",
  "version": "0.0.1",
  "publisher": "Jason Pang",
  "repository": "https://github.com/jasonpang/jarvis",
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "build/vscode-client/src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jarvis.show-status-notification",
        "title": "Jarvis Test"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.4",
    "@types/vscode": "^1.48.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.1",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "source-map-support": "^0.5.19",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "@metarhia/jstp": "^2.3.0",
    "install": "^0.13.0",
    "npm": "^6.14.8"
  }
}
